<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Author</key>
	<string><%= config[:author] %></string>
	<key>Builder Version</key>
	<string>8536.30.1</string>
	<key>CFBundleDisplayName</key>
	<string><%= config[:title] %></string>
	<key>CFBundleIdentifier</key>
	<string>com.dominionstrategy.safari.<%= config[:name] %></string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleShortVersionString</key>
	<string><%= config[:version] %></string>
	<key>CFBundleVersion</key>
	<string><%= config[:version] %></string>
	<key>Chrome</key>
	<dict/>
	<key>Content</key>
	<dict>
		<key>Scripts</key>
		<dict>
			<key>End</key>
			<array>
                <string>safari_libs.js</string>
				<string><%= config[:js].join("</string>\n				<string>") %></string>
			</array>
		</dict>
		<key>Stylesheets</key>
		<array>
			<string><%= config[:css].join("</string>\n			<string>") %></string>
		</array>
		<key>Whitelist</key>
		<array>
			<string><%= config[:targeturls].join("</string>\n			<string>") %></string>
		</array>
	</dict>
	<key>Description</key>
	<string><%= config[:desc] %></string>
	<key>ExtensionInfoDictionaryVersion</key>
	<string>1.0</string>
	<key>Permissions</key>
	<dict>
		<key>Website Access</key>
		<dict>
			<key>Allowed Domains</key>
			<array>
				<string>*.goko.com</string>
			</array>
			<key>Include Secure Pages</key>
			<true/>
			<key>Level</key>
			<string>Some</string>
		</dict>
	</dict>
	<key>URL</key>
	<string><%= config[:safarixtz_url] %></string>
	<key>Website</key>
	<string><%= config[:homepage] %></string>
    <key>Update Manifest URL</key>
    <string>https://<%= config[:hostServer] %>:<%= config[:hostPort] %><%= config[:hostURLBase] %><%= config[:extupdate][:safari] %></string>
</dict>
</plist>
